{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "comma-dangle": "off",
        "eol-last": "off",
        "require-jsdoc": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}